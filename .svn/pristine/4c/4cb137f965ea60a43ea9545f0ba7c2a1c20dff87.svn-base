<view class='container'>
  <image class='image' src='../../images/icons/register.png'></image>
  <!--表单提交-->
  <form bindsubmit='formSubmit'>
    <view class='border'>
      <view class='row'>
        <text class='text' decode='true'>账&emsp;&emsp;号：</text>
        <input placeholder='请输入账号' name='uid'></input>
      </view>
      <view class='line'></view>
      <view class='row'>
        <text class='text' decode='true'>密&emsp;&emsp;码：</text>
        <input placeholder='请输入密码' password='true' name='pwd'></input>
      </view>
      <view class='line'></view>
      <view class='row'>
        <text class='text'>确认密码：</text>
        <input placeholder='请确认密码' password='true' name='repwd'></input>
      </view>
    </view>
    <view>
      <button class='button' type='primary' form-type='submit'>注册</button>
    </view>
  </form>

</view>